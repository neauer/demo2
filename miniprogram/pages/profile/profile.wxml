      <i-card title="顺丰快递" extra="代取费用：2元" thumb="../../../imgs/shunfeng.jpeg">
        <view slot="content">送达地址：北八</view>
         <view slot="content">备注：无</view>
      </i-card>
      <van-button type="info" size='small' class="btn">帮忙代取</van-button>
    </view>
  </block>
<!-- 订单列表结束 -->

