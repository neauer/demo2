<view class='headerSwiper'>
  <swiper 
    indicator-dots='true'
    indicator-color='#333' 
    indicator-active-color='#f30' 
    autoplay='true'
    circular="{{circular}}" 
    interval="{{interval}}"
    duration="{{duration}}"
  >
    <view wx:for="{{imgUrls}}" wx:key="{{index}}">
      <swiper-item>
        <image src="{{item}}" class="slide-image"  />
      </swiper-item>
    </view>

  </swiper>
</view>
<!-- 轮播图结束 -->

<!-- 订单列表开始 -->
  <block>
